{
  "name": "vscode-tmux-keybinding",
  "displayName": "Tmux Keybinding",
  "description": "A simple extension for tmux behavior in vscode terminal.",
  "icon": "images/icon.png",
  "version": "0.0.7",
  "engines": {
    "vscode": "^1.43.0"
  },
  "categories": [
    "Keymaps"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/StephLin/vscode-tmux-keybinding.git"
  },
  "publisher": "stephlin",
  "contributes": {
    "keybindings": [
      {
        "key": "ctrl+b c",
        "command": "workbench.action.terminal.newInActiveWorkspace",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b 1",
        "command": "workbench.action.terminal.focusAtIndex1",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b 2",
        "command": "workbench.action.terminal.focusAtIndex2",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b 3",
        "command": "workbench.action.terminal.focusAtIndex3",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b 4",
        "command": "workbench.action.terminal.focusAtIndex4",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b 5",
        "command": "workbench.action.terminal.focusAtIndex5",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b 6",
        "command": "workbench.action.terminal.focusAtIndex6",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b 7",
        "command": "workbench.action.terminal.focusAtIndex7",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b 8",
        "command": "workbench.action.terminal.focusAtIndex8",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b 9",
        "command": "workbench.action.terminal.focusAtIndex9",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+k",
        "command": "workbench.action.terminal.clear",
        "when": "terminalFocus"
      },
      {
        "key": "ctrl+b shift+5",
        "command": "workbench.action.terminal.split",
        "when": "terminalFocus && panelPosition == 'bottom' && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b shift+'",
        "command": "workbench.action.terminal.split",
        "when": "terminalFocus && panelPosition != 'bottom' && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+shift+5",
        "command": "-workbench.action.terminal.split",
        "when": "terminalFocus"
      },
      {
        "key": "ctrl+b alt+up",
        "command": "workbench.action.terminal.resizePaneUp",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b alt+down",
        "command": "workbench.action.terminal.resizePaneDown",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b alt+left",
        "command": "workbench.action.terminal.resizePaneLeft",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b alt+right",
        "command": "workbench.action.terminal.resizePaneRight",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b ctrl+b",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b d",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b w",
        "command": "workbench.action.quickOpenTerm",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b z",
        "command": "workbench.action.toggleMaximizedPanel",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "shift+right",
        "command": "workbench.action.terminal.focusNext",
        "when": "terminalFocus"
      },
      {
        "key": "shift+left",
        "command": "workbench.action.terminal.focusPrevious",
        "when": "terminalFocus"
      },
      {
        "key": "ctrl+b right",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus && panelPosition == 'bottom' && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b left",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus && panelPosition == 'bottom' && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b down",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus && panelPosition != 'bottom' && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b up",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus && panelPosition != 'bottom' && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+b x",
        "command": "workbench.action.terminal.kill",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlBPrefix"
      },
      {
        "key": "ctrl+a c",
        "command": "workbench.action.terminal.newInActiveWorkspace",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a 1",
        "command": "workbench.action.terminal.focusAtIndex1",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a 2",
        "command": "workbench.action.terminal.focusAtIndex2",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a 3",
        "command": "workbench.action.terminal.focusAtIndex3",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a 4",
        "command": "workbench.action.terminal.focusAtIndex4",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a 5",
        "command": "workbench.action.terminal.focusAtIndex5",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a 6",
        "command": "workbench.action.terminal.focusAtIndex6",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a 7",
        "command": "workbench.action.terminal.focusAtIndex7",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a 8",
        "command": "workbench.action.terminal.focusAtIndex8",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a 9",
        "command": "workbench.action.terminal.focusAtIndex9",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a shift+5",
        "command": "workbench.action.terminal.split",
        "when": "terminalFocus && panelPosition == 'bottom' && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a shift+'",
        "command": "workbench.action.terminal.split",
        "when": "terminalFocus && panelPosition != 'bottom' && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a alt+up",
        "command": "workbench.action.terminal.resizePaneUp",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a alt+down",
        "command": "workbench.action.terminal.resizePaneDown",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a alt+left",
        "command": "workbench.action.terminal.resizePaneLeft",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a alt+right",
        "command": "workbench.action.terminal.resizePaneRight",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a ctrl+b",
        "command": "workbench.action.toggleSidebarVisibility",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a d",
        "command": "workbench.action.terminal.toggleTerminal",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a w",
        "command": "workbench.action.quickOpenTerm",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a z",
        "command": "workbench.action.toggleMaximizedPanel",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a right",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus && panelPosition == 'bottom' && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a left",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus && panelPosition == 'bottom' && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a down",
        "command": "workbench.action.terminal.focusNextPane",
        "when": "terminalFocus && panelPosition != 'bottom' && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a up",
        "command": "workbench.action.terminal.focusPreviousPane",
        "when": "terminalFocus && panelPosition != 'bottom' && config.tmuxKeybinding.enableCtrlAPrefix"
      },
      {
        "key": "ctrl+a x",
        "command": "workbench.action.terminal.kill",
        "when": "terminalFocus && config.tmuxKeybinding.enableCtrlAPrefix"
      }
    ],
    "configuration": {
      "title": "Tmux Keybinding",
      "properties": {
        "tmuxKeybinding.enableCtrlBPrefix": {
          "type": "boolean",
          "default": true,
          "description": "Enable keybindings that use Ctrl+B as a prefix."
        },
        "tmuxKeybinding.enableCtrlAPrefix": {
          "type": "boolean",
          "default": false,
          "description": "Enable keybindings that use Ctrl+A as a prefix."
        }
      }
    }
  }
}
